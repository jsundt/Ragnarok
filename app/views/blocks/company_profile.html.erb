      </div>

      <div class="col-xs-4">
        <section class="w-card u-margin-b-0" style="max-height: 190px; overflow: hidden;">
          <div class="w-slice u-bg--charcoal-115 u-padding-y-20">
            <p class="c-text--lead">Documents</p>

            <article class="w-card u-bg--charcoal">
              <p class="c-text--lead">Doc 1</p>
            </article>

            <article class="w-card u-bg--charcoal">
              <p class="c-text--lead">Doc 2</p>
            </article>

            <article class="w-card u-bg--charcoal">
              <p class="c-text--lead">Doc 3</p>
            </article>

            <article class="w-card u-bg--charcoal">
              <p class="c-text--lead">Doc 4</p>
            </article>
          </div>
        </section>
      </div>

      <div class="col-xs-4">
        <section class="w-card u-margin-b-0" style="max-height: 190px; overflow: hidden;">
          <div class="w-slice u-bg--charcoal-115 u-padding-y-20">
            <p class="c-text--lead">Notes</p>

            <article class="w-card u-bg--charcoal">
              <p class="c-text--lead">Note 1</p>
            </article>

            <article class="w-card u-bg--charcoal">
              <p class="c-text--lead">Note 2</p>
            </article>

            <article class="w-card u-bg--charcoal">
              <p class="c-text--lead">Note 3</p>
            </article>

            <article class="w-card u-bg--charcoal">
              <p class="c-text--lead">Note 4</p>
            </article>
          </div>
        </section>
      </div>
    </div>
  </div>
</header>

